<?xml version="1.0" encoding="UTF-8"?>
<du:dumonitorXPathConfiguration xmlns:du="xpath_conf_schema.xsd">
    <du:namespaces>
        <du:namespace>
            <du:prefix>soapenv</du:prefix>
            <du:uri>http://schemas.xmlsoap.org/soap/envelope/</du:uri>
        </du:namespace>
        <du:namespace>
            <du:prefix>xro</du:prefix>
            <du:uri>http://x-road.eu/xsd/xroad.xsd</du:uri>
        </du:namespace>
        <du:namespace>
            <du:prefix>iden</du:prefix>
            <du:uri>http://x-road.eu/xsd/identifiers</du:uri>
        </du:namespace>
        <du:namespace>
            <du:prefix>prod</du:prefix>
            <du:uri>http://producer.x-road.eu</du:uri>
        </du:namespace>
    </du:namespaces>
    <du:filters>
        <du:filter>
            <du:xpath>/soapenv:Envelope/soapenv:Header/xro:service/iden:serviceCode = 'getPersonData' and
                /soapenv:Envelope/soapenv:Header/xro:service/iden:serviceVersion = 'v1'
            </du:xpath>
            <du:loggableFields>
                <du:loggableField>
                    <du:xpath>/soapenv:Envelope/soapenv:Body/prod:getPersonDataResponse/response/personCode</du:xpath>
                    <du:fieldName>personcode</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>'Get Person Data'</du:xpath>
                    <du:fieldName>action</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>'Test AK'</du:xpath>
                    <du:fieldName>sender</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>'Test receiver'</du:xpath>
                    <du:fieldName>receiver</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>'A'</du:xpath>
                    <du:fieldName>restrictions</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>/soapenv:Envelope/soapenv:Header/xro:service/iden:memberCode</du:xpath>
                    <du:fieldName>sendercode</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>/soapenv:Envelope/soapenv:Header/xro:client/iden:memberCode</du:xpath>
                    <du:fieldName>receivercode</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>'getPersonData.v1'</du:xpath>
                    <du:fieldName>actioncode</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>/soapenv:Envelope/soapenv:Header/xro:id</du:xpath>
                    <du:fieldName>xroadrequestid</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>/soapenv:Envelope/soapenv:Header/xro:service/iden:serviceCode</du:xpath>
                    <du:fieldName>xroadservice</du:fieldName>
                </du:loggableField>
                <du:loggableField>
                    <du:xpath>/soapenv:Envelope/soapenv:Header/xro:userId</du:xpath>
                    <du:fieldName>usercode</du:fieldName>
                </du:loggableField>
            </du:loggableFields>
        </du:filter>
    </du:filters>
</du:dumonitorXPathConfiguration>